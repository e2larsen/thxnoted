<div class="container">
      <h1>Pricing</h1>
  </div>
</div>
<div class="container">
<link href="http://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
  

<div class="card-deck-wrapper">
  <div class="card-deck">
    <div class="card">
      <div class="card-block text-xs-center">
        <h4 class="card-title">One Card</h4>
        <span class="icon yellow"><i class="fa fa-usd"></i></span>
        <span class="price-large blue">5</span>
        <p class="card-text">One hand written thank you card</p>
        <button type="button" class="btn btn-info">Choose</button>
      </div>
    </div>
    <div class="card">
      <div class="card-block text-xs-center">
        <h4 class="card-title">Five Cards</h4>
        <span class="icon yellow"><i class="fa fa-usd"></i></span>
        <span class="price-large yellow">25</span>
        <p class="card-text">Five hand written thank you cards</p>
        <button type="button" class="btn btn-warning">Choose</button>
      </div>
    </div>
    <div class="card">
      <div class="card-block text-xs-center">
        <h4 class="card-title">Ten Cards</h4>
        <span class="icon yellow"><i class="fa fa-usd"></i></span>
        <span class="price-large green">50</span>
        <p class="card-text">Ten hand written thank you cards</p>
        <button type="button" class="btn btn-success">Choose</button>
      </div>
    </div>
  </div>
</div>